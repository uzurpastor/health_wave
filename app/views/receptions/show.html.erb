<%= @reception.inspect %>
<br>
<% if current_user %>
  <%= link_to "List of receptions for user", users_index_reception_path %>
<% elsif current_doctor %>
  <% if @reception.process? %>
    <%= link_to 'set time of meet', doctors_new_time_receptions_path %>    
